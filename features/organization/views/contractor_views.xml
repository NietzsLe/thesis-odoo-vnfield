                    <!--
                    ‚îå‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îê
                    ‚îÇ   üîé T√çCH H·ª¢P OWL COMPONENT: ContractorFilter‚îÇ
                    ‚îÇ   Hi·ªÉn th·ªã filter UI cho contractor         ‚îÇ
                    ‚îî‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îò
                    -->
                    <t t-name="kanban-box">
                        <!--
                        üìù TODO(assistant): T·∫•t c·∫£ gi√° tr·ªã hi·ªÉn th·ªã ph·∫£i l·∫•y t·ª´ bi·∫øn t-set, kh√¥ng truy xu·∫•t tr·ª±c ti·∫øp t·ª´
                        record trong t-out.
                        -->
                        <t t-set="name" t-value="record.name.value" />
                        <t t-set="description" t-value="record.description.value or ''" />
                        <t t-set="contractor_type" t-value="record.contractor_type.raw_value" />
                        <t t-set="user_count"
                            t-value="record.user_ids.raw_value ? record.user_ids.raw_value.length : 0" />
                        <t t-set="external_id"
                            t-value="record.external_id and record.external_id.value or '-'" />
                        <!--
                        ‚îå‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îê
                        ‚îÇ   üü¶ V√ôNG M√É: KANBAN CARD CONTRACTOR        ‚îÇ
                        ‚îÇ   M·ª•c ƒë√≠ch: Hi·ªÉn th·ªã th·∫ª contractor v·ªõi:    ‚îÇ
                        ‚îÇ   - Chip style cho contractor_type          ‚îÇ
                        ‚îÇ   - Conditional display cho external info   ‚îÇ
                        ‚îÇ   - Clickable ƒë·ªÉ m·ªü form                    ‚îÇ
                        ‚îÇ   - Registration status cho external        ‚îÇ
                        ‚îî‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îò
                        -->
                        <!--
                        üí° NOTE(assistant): 
                        - oe_kanban_global_click class b√™n trong div l√†m card clickable ƒë·ªÉ m·ªü form
                        - Cursor pointer ƒë·ªÉ hi·ªÉn th·ªã card c√≥ th·ªÉ click
                        - Registration status ch·ªâ hi·ªÉn th·ªã cho external contractor
                        - User count ƒë∆∞·ª£c t√≠nh t·ª´ user_ids.value.length
                        -->
                        <div class="oe_kanban_card"
                            style="background: #fff; border: 1px solid #ddd; padding: 12px; cursor: pointer; border-radius: 12px;">
                            <!-- Global click to open record -->
                            <div class="oe_kanban_global_click"></div>
                            <!-- Header v·ªõi t√™n v√† chip type -->
                            <div
                                style="display: flex; align-items: center; justify-content: space-between; margin-bottom: 8px;">
                                <span style="font-weight: bold; font-size: 1.1em; color: #333;">
                                    <t t-out="name" />
                                </span>
                                <!-- üéØ CHIP STYLE cho contractor_type - ch·ªâ hi·ªÉn th·ªã ti·∫øng Vi·ªát -->
                                <t t-if="contractor_type == 'internal'">
                                    <span class="badge"
                                        style="padding: 4px 12px; border-radius: 12px; color: #fff; font-size: 0.85em; font-weight: 500; background: #4caf50;">N·ªôi
                                        b·ªô</span>
                                </t>
                                <t t-elif="contractor_type == 'external'">
                                    <span class="badge"
                                        style="padding: 4px 12px; border-radius: 12px; color: #fff; font-size: 0.85em; font-weight: 500; background: #2196f3;">B√™n
                                        ngo√†i</span>
                                </t>
                                <t t-elif="contractor_type == 'shared'">
                                    <span class="badge"
                                        style="padding: 4px 12px; border-radius: 12px; color: #fff; font-size: 0.85em; font-weight: 500; background: #ff9800;">Li√™n
                                        nh√† th·∫ßu</span>
                                </t>
                                <t t-else="">
                                    <span class="badge"
                                        style="padding: 4px 12px; border-radius: 12px; color: #fff; font-size: 0.85em; font-weight: 500; background: #999;">
                                        Debug: <t t-out="contractor_type" />
                                    </span>
                                </t>
                            </div>

                            <!-- M√¥ t·∫£ v·ªõi gi·ªõi h·∫°n 2 d√≤ng v√† d·∫•u ... -->
                            <div t-if="description" style="margin-bottom: 8px;">
                                <div
                                    style="color: #555; font-size: 0.85em; line-height: 1.3; 
                                    overflow: hidden; display: -webkit-box; -webkit-line-clamp: 2; 
                                    -webkit-box-orient: vertical; word-wrap: break-word;">
                                    <t t-out="description" />
                                </div>
                            </div>

                            <!-- üë• Danh s√°ch nh√¢n vi√™n v·ªõi avatar -->
                            <div style="margin: 8px 0;">
                                <div style="display: flex; align-items: center;">
                                    <span style="color: #888; font-size: 0.95em; margin-right: 8px;">Nh√¢n
                                        s·ª±:</span>
                                    <div style="display: flex; align-items: center;">
                                        <t t-if="user_count > 0">
                                            <field name="user_ids" widget="many2many_avatar_user"
                                                options="{'size': 'small', 'display_name': false, 'limit': 2}"
                                                style="margin-right: 8px;" />
                                        </t>
                                    </div>
                                </div>
                            </div>
                            <!-- üìä T·ªïng s·ªë d·ª± √°n v√† teams -->
                            <div style="display: flex; gap: 16px; margin-bottom: 8px;">
                                <div>
                                    <span style="color: #888; font-size: 0.95em;">D·ª± √°n:</span>
                                    <span
                                        style="color: #1976d2; font-weight: bold; margin-left: 4px;">
                                        <t t-out="record.project_count.value || 0" />
                                    </span>
                                </div>
                                <div>
                                    <span style="color: #888; font-size: 0.95em;">Teams:</span>
                                    <span
                                        style="color: #43a047; font-weight: bold; margin-left: 4px;">
                                        <t t-out="record.team_count.value || 0" />
                                    </span>
                                </div>
                            </div>

                            <!-- üéØ CONDITIONAL DISPLAY: External ID hi·ªÉn th·ªã cho external v√† shared -->
                            <div t-if="contractor_type in ['external', 'shared'] and external_id"
                                style="margin-bottom: 6px;">
                                <span style="color: #666; font-size: 0.95em;">External ID: </span>
                                <span style="font-weight: bold; color: #333;">
                                    <t t-out="external_id" />
                                </span>
                            </div>
                        </div>
                    </t>
                </templates>
            </kanban>
        </field>
    </record>

    <!-- Contractor Form View -->
    <record id="view_contractor_form" model="ir.ui.view">
        <field name="name">contractor.form</field>
        <field name="model">vnfield.contractor</field>
