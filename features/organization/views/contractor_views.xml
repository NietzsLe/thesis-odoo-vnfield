                            <field name="director_id_readonly" invisible="1" />
                            <field name="director_id" options="{'no_create': True}"
                                readonly="director_id_readonly" />
                            <field name="project_director_ids_readonly" invisible="1" />
                            <field name="bidding_manager_ids_readonly" invisible="1" />
                            <field name="project_director_ids" widget="many2many_tags"
