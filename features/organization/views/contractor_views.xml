    <!-- Contractor Search View (b·ªô l·ªçc b√™n tr√°i) -->
    <record id="view_contractor_search" model="ir.ui.view">
        <field name="name">contractor.search</field>
        <field name="model">vnfield.contractor</field>
        <field name="priority" eval="16" />
        <field name="arch" type="xml">
            <!--
            ‚îå‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îê
            ‚îÇ   üîé SEARCH VIEW: CONTRACTOR                ‚îÇ
            ‚îÇ   M·ª•c ƒë√≠ch: T√¨m ki·∫øm v√† l·ªçc contractor      ‚îÇ
            ‚îÇ   Ch·ªâ d√πng tr∆∞·ªùng l∆∞u tr·ªØ, kh√¥ng d√πng computed‚îÇ
            ‚îî‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îò
            -->
            <search>
                <field name="name" />
                <field name="external_id" />
                <filter string="Internal" name="internal"
                    domain="[('contractor_type','=','internal')]" />
                <filter string="External" name="external"
                    domain="[('contractor_type','=','external')]" />
                <filter string="Registered" name="registered" domain="[('external_id','!=',False)]" />
                <filter string="Not Registered" name="not_registered"
                    domain="[('external_id','=',False)]" />
            </search>
        </field>
    </record>

    <!-- Contractor Action & Menu -->
    <record id="action_contractor" model="ir.actions.act_window">
        <field name="name">Contractors</field>
        <field name="res_model">vnfield.contractor</field>
        <field name="view_mode">kanban,tree,form</field>
        <field name="context">{}</field>
        <field name="domain">[]</field>
        <field name="view_ids"
            eval="[
            (5, 0, 0),
