                <templates>
                    <!-- Create Button Template -->
                    <t t-name="kanban-menu">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-6 col-md-4 col-lg-3">
                                    <a type="object" name="create"
                                        class="btn btn-primary btn-sm btn-block">
